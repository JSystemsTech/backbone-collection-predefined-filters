  "description": "An extention of Backbone Collection that allows for predefined filter(s) to be added/removed/applied/unapplied on the collection",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require mocha-helper.js --recursive src/**/*-spec.js",
    "unit": "mocha -R mocha-spec-json-reporter --require mocha-helper.js --recursive src",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R mocha-spec-json-reporter src/**/*-spec.js",
    "report": "node_modules/istanbul/lib/cli.js report json-summary",
    "document": "gulp buildreadme"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JSystemsTech/backbone-collection-predefined-filters.git"
  },
  "engines": {
    "node": ">=4.2.4"
  },
  "keywords": [
    "backbone",
    "backbone-collection",
    "backbone-collection-predefined-filters",
    "backbone-filters",
    "backbone-filtering",
    "backbone-collection-filters",
    "backbone-collection-filtering"
  ],
  "author": "JSystemsTech <jsystemstech@gmail.com> (http://jsystemstech.com)",
  "dependencies": {
    "async": "^2.1.2",
    "backbone": "^1.2.2",
    "drilldown": "^0.1.1",
    "moment": "^2.15.1",
    "underscore": "^1.6.9"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JSystemsTech/backbone-collection-predefined-filters/issues"
  },
  "homepage": "https://github.com/JSystemsTech/backbone-collection-predefined-filters#readme",
  "devDependencies": {
    "browserify": "^13.1.0",
    "comment-parser": "^0.4.0",
    "coveralls": "^2.11.14",
    "event-stream": "3.3.4",
    "gh-badges": "^1.3.0",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-flatmap": "^1.0.0",
    "gulp-istanbul": "^1.1.1",
    "gulp-mocha": "^3.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "istanbul": "^0.4.5",
    "minimist": "^1.2.0",
    "mocha": "^3.1.2",
